<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.MenuScreenController">
   <top>
      <MenuBar nodeOrientation="LEFT_TO_RIGHT" prefHeight="29.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <menus>
            <Menu mnemonicParsing="false" text="Opiekunowie">
               <items>
                  <MenuItem mnemonicParsing="false" onAction="#handleOpiekunowieDodaj" text="Dodaj" />
                  <MenuItem mnemonicParsing="false" onAction="#handleOpiekunowieUsun" text="Usun" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Pielegniarki">
               <items>
                  <MenuItem mnemonicParsing="false" text="Dodaj" />
                  <MenuItem mnemonicParsing="false" text="Usun" />
               </items>
            </Menu>
         </menus>
      </MenuBar>
   </top>
</BorderPane>
